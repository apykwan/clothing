  "main": "server.js",
  "scripts": {
    "client": "npm start --prefix client",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run server client\"",
